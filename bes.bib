@Article{Roda2012,
  Title                    = {Autismo a scuola: il valore dell'inclusione},
  Author                   = {Graziella Roda},
  Date                     = {2010-05-22},
  Journaltitle             = {Studi e Documenti},
  Year                     = {2012},
  ISSN                     = {2282-2151},
  Month                    = {3},
  Note                     = {USR per L'Emila Romagna \url{http://ww2.istruzioneer.it}},
  Number                   = {3},
  Series                   = {Studi e Documenti},
  Subtitle                 = {Parte I - Educazione e integrazione, due diritti complementari},
  Url                      = {http://ww2.istruzioneer.it/wp-content/uploads/2011/09/Roda-1-3-2012.pdf},

  Abstract                 = {La conferenza dal titolo ``Educazione e integrazione, due diritti complementari'' è stata svolta in occasione del primo seminario “Autismo ed educazione. Il ruolo della scuola” tenutosi a Fidenza (Parma) il 22 maggio 2010. Il seminario è stato organizzato congiuntamente dall'Associazione Nazionale Genitori Soggetti Autistici (ANGSA) Emilia-Romagna e dall'Ufficio Scolastico Regionale per l'Emilia-Romagna Ambito territoriale di Parma. In tale contesto è stata presentata una riflessione sul ruolo dell'Analisi Applicata al Comportamento (Applied Behavior Analysis) per l'abilitazione dei bambini con diagnosi autismo - disturbi pervasivi dello sviluppo nell'ottica del modello italiano di educazione integrata. I percorsi ABA sono infatti nati e si sono sviluppati nei paesi anglosassoni, negli USA in particolare, in contesti di educazione speciale o di scuole speciali o, in molti casi, in trattamenti ambulatoriali specializzati. Il trasferimento di modelli di intervento da un contesto culturale e sociale ad un altro richiedono riflessioni ed azioni, e ricerche, di grande ponderatezza. L'invito rivolto ai docenti, alle famiglie ed agli analisti comportamentali, è quello di imparare a lavorare insieme in un contesto culturale specificamente connotato, come quello italiano, modificando sia le modalit\'{a} di insegnamento ai ragazzi autistici, sia le modalit\'{a} di abilitazione dei medesimi ragazzi. Si tratta di coniugare gli stimoli provenienti da percorsi di provata efficacia realizzati in diversi contesti, senza per questo rinunciare ai principi fondanti della pedagogia per l'integrazione sviluppati nel nostro Paese},
  Keywords                 = {risorsa,104,autismo},
  Owner                    = {claudio},
  Timestamp                = {2013.06.30}
}

@Article{Roda2012a,
  Title                    = {Autismo a scuola: il valore dell'inclusione},
  Author                   = {Graziella Roda},
  Date                     = {2011-06-11},
  Journaltitle             = {Studi e Documenti},
  Year                     = {2012},
  ISSN                     = {2282-2151},
  Month                    = {3},
  Note                     = {USR per L'Emila Romagna \url{http://ww2.istruzioneer.it}},
  Number                   = {3},
  Series                   = {Studi e Documenti},
  Url                      = {http://ww2.istruzioneer.it/wp-content/uploads/2011/09/Roda-2-3-2012.pdf},

  Abstract                 = {La conferenza dal titolo “L'Io e gli altri (chiedendo scusa a Laing)”è stata tenuta in occasione del secondo seminario “Autismo ed educazione: il valore dell'inclusione” tenutosi a Fidenza (Parma) l'11 giugno 2011. Anche questo secondo seminario è stato organizzato congiuntamente dall'Associazione Nazionale Genitori Soggetti Autistici (ANGSA) Emilia-Romagna e dall'Ufficio Scolastico per l'Emilia-Romagna, ambito territoriale di Parma. La conferenza si è aperta riprendendo le parole con cui si era chiusa la conferenza precedente, tenutasi il 22 maggio 2010, che si era incentrata sul rapporto tra la scuola integrata italiana e i modelli di intervento comportamentali di abilitazione dei ragazzi autistici o con disturbi pervasividello sviluppo, genericamente indicati come A.B.A. (Applied Behavior Analysis). Nel corso del seminario è stato presentato il percorso di abilitazione di un bambino autistico dalla scuola dell'infanzia alla scuola primaria, documentando come attraverso l'analisi applicata al comportamento sia stato possibile dare a questo bambino la possibilit\'{a} di fruire della scuola comune, avendo egli acquisito modalit\'{a} di comunicazione, di relazione, di funzionamento cognitivo tali da rendere proficuo il rapporto con i compagni. La riflessione che segue raccoglie alcuni “buoni esempi” di lavoro scolastico, presentando situazioni in cui grazie alla scuola integrata è stato possibile affrontare difficolt\'{a}, scoprire talenti inaspettati ed educare i compagni al rapporto con ragazzi autistici},
  Keywords                 = {risorsa,104,autismo},
  Owner                    = {claudio},
  Timestamp                = {2013.06.30}
}

